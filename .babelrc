{
  "presets": [
    "es2015",
    "react",
    "stage-3"
  ],
  "plugins": [
    "babel-plugin-rewire",
    "inline-react-svg",
    "transform-runtime",
    ["module-resolver", {
      "root": ["./"]
    }],
    "transform-class-properties"
  ],
  "env": {
    "development": {
      "plugins": [
        ["babel-plugin-styled-components", {
          "ssr": true
        }]
      ]
    }
  }
}
