  "prettier": {
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.mjs?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.coffee$": "<rootDir>/node_modules/jest-coffee-preprocessor/index.js",
      ".(ts|tsx|mjs)": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "/test/.*\\.test\\.(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "coffee",
      "js",
      "json",
      "jsx",
      "mjs",
      "ts",
      "tsx"
    ],
    "setupFiles": [
      "<rootDir>/test.config.js"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text-summary"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "report-dir": "./.nyc_output/",
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".coffee",
      ".js"
    ],
    "lines": 0
  },
  "lint-staged": {
    "*.@(ts|tsx|js|jsx)": [
      "yarn lint --fix",
      "yarn prettier-write",
      "git add"
    ],
    "*.json": [
      "yarn prettier-write"
    ]
  },
